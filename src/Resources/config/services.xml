<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
                               https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="manuxi_sulu_tweaks.admin" class="Manuxi\SuluTweaksBundle\Admin\TweaksAdmin">
            <argument type="service" id="sulu_admin.view_builder_factory"/>
            <argument>%sulu_tweaks.publish_state_indicator.enable_offset%</argument>
            <argument>%sulu_tweaks.publish_state_indicator.offset_width%</argument>
            <tag name="sulu.admin"/>
            <tag name="sulu.context" context="admin"/>
        </service>
    </services>
</container>